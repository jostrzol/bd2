<userDefinedSQL><![CDATA[SELECT
    os.id_osoby,
    os.email,
    os.nazwisko,
    os.imie,
    AVG(o.ocena) AS srednia_ocena
FROM
         odpowiedzi o
    INNER JOIN ankiety a ON ( o.id_ankiety = a.id_ankiety )
    INNER JOIN osoby   os ON ( a.id_agenta = os.id_osoby )
GROUP BY
    os.id_osoby,
    os.email,
    os.nazwisko,
    os.imie]]></userDefinedSQL>
